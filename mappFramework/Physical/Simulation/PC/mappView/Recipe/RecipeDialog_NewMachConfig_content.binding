            <Source xsi:type="opcUa" refId="::RecipeMgr:HmiRecipe.Parameters.FileName" attribute="value" />
            <Target xsi:type="brease" contentRefId="RecipeDialog_NewMachConfig_content" widgetRefId="TextInNewRecipe" attribute="value" />
        </Binding>
    </Bindings>
</BindingsSet>
