          <Step order="1">
            <Action>
              <Target xsi:type="widgets.brease.NumericInput.Action" contentRefId="FileDialog_FIFO_dialog_content" widgetRefId="NumInMaxNumFiles">
                <Method xsi:type="widgets.brease.NumericInput.Action.SetVisible" value="false" />
