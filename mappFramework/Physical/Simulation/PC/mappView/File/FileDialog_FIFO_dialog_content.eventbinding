          </Step>
          <Step order="2">
            <Action>
              <Target xsi:type="widgets.brease.Label.Action" contentRefId="FileDialog_FIFO_dialog_content" widgetRefId="LabelMaxFolderSize">
                <Method xsi:type="widgets.brease.Label.Action.SetVisible" value="true" />
