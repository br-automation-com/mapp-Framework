          </Step>
          <Step order="6">
            <Action>
              <Target xsi:type="session.VariableAction" refId="SessionVarFifoFolderSize">
                <Method xsi:type="session.VariableAction.SetValueNumber" value="=MaxFolderSize" />
              </Target>
            </Action>
          </Step>
          <Step order="7">
