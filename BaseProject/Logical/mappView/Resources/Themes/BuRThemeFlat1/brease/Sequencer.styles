		textColor="#646464" 
		backColor="transparent" 
		borderStyle="none" 
		margin="5px"
	padding="0px" />
	
	<Style id="seq-numeric-in" xsi:type="widgets.brease.NumericInput" 
		fontSize="36px"
		margin="5px"
		unitFontSize="24px"
