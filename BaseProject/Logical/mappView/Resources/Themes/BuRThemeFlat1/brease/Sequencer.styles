		borderWidth="0px"
		backColor="#ff1a1a"
		activeBackColor="#FF8500"
	/> 
	<Style id="StepItem_Blue" xsi:type="widgets.brease.SequencerStepItem" 
		margin="20px"
		textColor="#FFFFFF"
		activeTextColor="#FFFFFF"
